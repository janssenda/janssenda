<service>
    <id>Huetron</id>
    <name>Huetron</name>
    <description>This runs Spring Boot as a Service.</description>
    <env name="Huetron_HOME" value="%BASE%"/>
    <executable>java</executable>
    <arguments>-Xmx256m -jar "%BASE%\Huetron.jar"</arguments>
    <logmode>rotate</logmode>
</service>